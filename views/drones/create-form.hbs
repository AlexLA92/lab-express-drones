<h2>Create a new drone</h2>
<form action="/drones/create" method = "POST">
    <label for = "name">Name</label>
    <input id = "name" name = "name" type="text">
    <label for = "propellers">Propellers</label>
    <input id = "propellers" name = "propellers" type="number">
    <label for = "maxSpeed">Max Speed</label>
    <input id = "maxSpeed" name = "maxSpeed" type="number">
    <input type = "submit" value = "Create drone">
</form>